{
  "name": "@bedrock/recaptcha-test",
  "version": "0.0.1-0",
  "private": true,
  "scripts": {
    "test": "node --preserve-symlinks test.js test",
    "coverage": "cross-env NODE_ENV=test nyc --reporter=lcov --reporter=text-summary npm test",
    "coverage-ci": "cross-env NODE_ENV=test nyc --reporter=lcovonly npm test",
    "coverage-report": "nyc report"
  },
  "dependencies": {
    "@bedrock/core": "^6.1.3",
    "@bedrock/express": "^8.3.0",
    "@bedrock/https-agent": "^4.1.0",
    "@bedrock/mongodb": "^11.0.0",
    "@bedrock/server": "^5.1.0",
    "@bedrock/test": "^8.2.0",
    "cross-env": "^7.0.3",
    "nyc": "^15.1.0"
  },
  "nyc": {
    "excludeNodeModules": false,
    "include": [
      "node_modules/bedrock-recatpcha-test/**"
    ],
    "exclude": [
      "node_modules/bedrock-recaptcha-test/node_modules/**"
    ]
  }
}
